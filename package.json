{
  "name": "ticketing-web-application",
  "version": "0.0.1",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:dev": "env-cmd -f ./environments/.env.development react-scripts start",
    "build:dev": "env-cmd -f ./environments/.env.development npm run-script build",
    "start:qa": "env-cmd -f ./environments/.env.qa react-scripts start",
    "build:qa": "env-cmd -f ./environments/.env.qa npm run-script build",
    "start:prod": "env-cmd -f ./environments/.env.production react-scripts start",
    "build:prod": "env-cmd -f ./environments/.env.production npm run-script build",
    "start:staging": "env-cmd -f ./environments/.env.staging react-scripts start",
    "build:staging": "env-cmd -f ./environments/.env.staging npm run-script build"
  },
  "dependencies": {
    "env-cmd": "^10.1.0",
    "history": "^4.10.1",
    "keycloak-js": "4.0.0-beta.2",
    "mdbreact": "^5.2.0",
    "react": "^16.12.0",
    "react-datepicker": "^4.5.0",
    "react-datetime-picker": "^3.4.3",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "html-webpack-plugin": "^3.2.0",
    "path": "^0.12.7",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
